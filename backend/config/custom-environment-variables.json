{
  "app": {
    "public_hostname": "ANOTEA_APP_PUBLIC_HOSTNAME",
    "mailer": {
      "limit": {
        "__name": "ANOTEA_APP_MAILER_LIMIT",
        "__format": "json"
      }
    }
  },
  "mongodb": {
    "uri": "ANOTEA_MONGODB_URI"
  },
  "security": {
    "secret": "ANOTEA_SECURITY_SECRET"
  },
  "auth": {
    "backoffice": {
      "secret": "ANOTEA_AUTH_BACKOFFICE_SECRET"
    },
    "esd": {
      "secret": "ANOTEA_AUTH_ESD_SECRET"
    },
    "kairos": {
      "secret": "ANOTEA_AUTH_KAIROS_SECRET"
    },
    "maformation": {
      "secret": "ANOTEA_AUTH_MAFORMATION_SECRET"
    }
  },
  "log": {
    "console": {
      "json": {
        "__name": "ANOTEA_LOG_CONSOLE_JSON",
        "__format": "json"
      }
    },
    "file": {
      "enabled": {
        "__name": "ANOTEA_LOG_FILE_ENABLED",
        "__format": "json"
      }
    },
    "sentry": {
      "enabled": {
        "__name": "ANOTEA_LOG_SENTRY_ENABLED",
        "__format": "json"
      },
      "DSN": "ANOTEA_LOG_SENTRY_DSN"
    }
  },
  "smtp": {
    "host": "ANOTEA_SMTP_HOST",
    "port": "ANOTEA_SMTP_PORT"
  },
  "front": {
    "googleSiteVerification": "ANOTEA_FRONT_GOOGLE_SITE_VERIFICATION"
  }
}