    "linebyline": "1.3.0",
    "md5": "2.2.1",
    "mocha": "5.2.0",
    "nodemon": "1.17.5",
    "rxjs": "5.5.11",
    "superagent": "3.8.3",
    "superagent-throttle": "0.2.5",
    "xml2js": "0.4.19"
  }
